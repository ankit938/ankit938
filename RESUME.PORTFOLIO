*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: sans-serif;
}
ul{
    list-style-type: none;
    margin: 0;
    padding: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    gap: 40px;

}
.container{
    width: 100%;
    height: 100vh;
    background-color: #3e4142;
    align-items: center;
    display: flex;
    justify-content: center;
    
}
.calc{
    background: #272d36;
    padding: 18px;
    border-radius: 12px;
    border: 5px solid #fff;
}
.calc form input{
    border: 0;
    outline: 0;
    width: 50px;
    height: 50px;
    border-radius: 10px;
    box-shadow: -7px -7px 15px rgba(255, 255, 255, 0.1), 5px 5px 15px rgba(0, 0, 0, 0.2);
   background: transparent;
   font-size: 19px;
   color: #fff;
   cursor: pointer;
   margin: 10px;
   
}
/* .calc form input:hover{
    width: 51px;
    height: 53px;
} */
form .display{
   display: flex;
   justify-content: flex-end;
   margin: 20px 0;
}
form .display input{
    text-align: right;
    flex: 1;
    font-size: 40px;
    box-shadow: none;
}
form input.equal{
    width: 132px;
}
    
    form input.givediffColor{
        color: #ecfc0d;
        font-size: 25px;
        font-weight:600;
    }



